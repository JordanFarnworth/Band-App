<div class="col-md-10 col-md-offset-1 well" id="new-event-page">
  <div class="row text-center">
    <h1>Create an Event</h1>
  </div>
  <form class="form-horizontal">
    <fieldset>
      <div class="form-group">
        <label for="event-title" class="col-lg-2 control-label">Title</label>
        <div class="col-lg-6">
          <input type="text" class="form-control" id="create-event-title" style="cursor: auto; background-image: url(data:image/png;base64,); background-attachment: scroll; background-position: 100% 50%; background-repeat: no-repeat;">
        </div>
      </div>
      <div class="form-group">
        <label for="textArea" class="col-lg-2 control-label">Description</label>
        <div class="col-lg-8">
          <textarea class="form-control" rows="3" id="create-event-description"></textarea>
        </div>
      </div>
      <div class="form-group">
        <label class="col-lg-2 control-label">Privacy</label>
        <div class="col-lg-10">
          <div class="radio">
            <label>
              <input type="radio" name="optionsRadios" id="optionsRadios1" value="option1" checked="">
              Public
            </label>
          </div>
          <div class="radio">
            <label>
              <input type="radio" name="optionsRadios" id="optionsRadios2" value="option2">
              Private
            </label>
          </div>
        </div>
      </div>
      <div class="form-group">
        <label class="col-lg-2 control-label" for="create-event-address">Address of Event</label>
        <div class="col-lg-6">
          <input type="text" class="form-control" id="create-event-address" >
        </div>
      </div>
      <div class="form-group">
        <label class="control-label col-lg-2" for="create-event-price">Offering Price</label>
        <div class="input-group col-lg-2">
          <span class="input-group-addon">$</span>
          <input id="create-event-price" class="form-control">
        </div>
      </div>
      <div class="form-group">
        <label class="col-lg-2 control-label" for="application-start">Start Date</label>
        <div class="col-lg-4">
          <input class="form-control" id="create-event-start">
        </div>
      </div>
      <div class="form-group">
        <label class="col-lg-2 control-label" for="application-end">End Date</label>
        <div class="col-lg-4">
          <input class="form-control" id="create-event-end">
        </div>
      </div>
    </fieldset>
  </form>
  <div class="row">
    <div class="col-md-offset-2">
      <button type="button" class="btn btn-default">Close</button>
      <button type="button" id="create-event-btn" class="btn btn-primary">Save changes</button>
    </div>
  </div>
</div>
