<% if @current_user.user_data[:user_entity_type] === 'none' %>
  <%= render 'pickentity' %>
<% elsif @current_user.user_data[:user_entity_type] === 'band' %>
  <%= render 'banddashboard' %>
<% elsif @current_user.user_data[:user_entity_type] === 'party' %>
  <%= render 'partydashboard' %>
<% end %>
